<body>
<nav class="main-nav navbar navbar-fixed-top navbar-default" role="navigation">
    <!--<a href="/" id="test" class="navbar-brand"><img src="/images/gSpace.jpg" alt="LOGO"/></a>-->
    <a href="/" class="navbar-brand"><img src="/images/orange2.svg" style="height:45px;width:185px;" alt="LOGO"/></a>
        <ul class="nav navbar-nav">

            <li class="nav-item">
                <a class="nav-link" href="/resources">Resources</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/gflow">gFlow</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/meetups">Meetups</a>
            </li>
            <li class="nav-item">
                <a class="nav-link" href="/projects">Projects</a>
            </li>
        </ul>
        <ul class="user-controls nav navbar-nav pull-right">
            {{#if user}}
                <div>
                    <ul class="nav navbar-nav">
                        <li>
                            <a class="navbar-brand nav-link" data-toggle="dropdown" href="#">
                                <img class="img-thumbnail" style="max-width:100%; height:25px; width:25px;"
                                     src="{{user._json.avatar_url}}" alt="user avatar"/>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="/users/profile">
                                        <i class="fa fa-sliders menu " aria-hidden="true"></i>
                                        <div class="nav-link">Edit Profile</div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-star-o" aria-hidden="true"></i>
                                        <div class="nav-link">Favorites</div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="#">
                                        <i class="fa fa-bell-o" aria-hidden="true"></i>
                                        <div class="nav-link">Notifications</div>
                                    </a>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <a href="/auth/github/logout" gi>
                                        <i class="fa fa-sign-out" aria-hidden="true"></i>
                                        <div class="nav-link">Logout</div>
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            {{else}}
                <li>
                    <a class="nav-link" href="/auth/github">Login with GitHub
                        <i class="fa fa-github-alt github-login" aria-hidden="true"></i>
                    </a>
                </li>
            {{/if}}
        </ul>
</nav>
<div class="container" style="margin-top: 100px;">
    {{{body}}}
</div>
<footer class="footer">
    <div class="container">
        <p class="text-muted">Designed and built with love by g[31]</p>
    </div>
</footer>
